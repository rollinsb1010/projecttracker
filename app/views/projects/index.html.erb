    $(function() {
        $( "#progressbar" ).progressbar({
            value: 37
        });
    });
    </script>
</head>
<body>
	<h1>New project</h1>

<%= render 'form' %>

<%= link_to 'Back', projects_path %>


		<%= semantic_form_for @projects do |f| %>
    <%= f.inputs do %>
       <%= f.input :project_name, :input_html => { :disabled => true } %>
       <%= f.input :due_date %>
       <%= f.input :components, :as => :radio %>
       <%= f.input :client_name %>
      <%= f.input :medium %>
    <% end %>
    <%= f.actions do %>
      <%= f.action :submit, :as => :button %>
      <%= f.action :cancel, :as => :link %>
    <% end %>
  <% end %>

