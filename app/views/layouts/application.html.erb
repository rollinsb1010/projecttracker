<!DOCTYPE html>
<html>
<head>
  <title>Projecttracker</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body id="<%= params[:controller].parameterize %>_controller">
	<div id="adminnav">
  <% if admin_signed_in? %>
    Signed in as <%= current_admin.email %>. Not you?
    <%= link_to "Sign out", destroy_admin_session_path %>
  <% else %>
    <%= link_to "Sign up", new_admin_registration_path %> or
    <%= link_to "Sign in", new_admin_session_path %>
  <% end %>
</div>

<%= yield %>

</body>
</html>
